 <VBox id="vbox2" class="SignupvBox2" width="40%" height="60%" justifyContent="SpaceBetween" alignContent="SpaceBetween" alignItems="Center" backgroundDesign="Solid">

           
             <VBox id="vbox3" class="loginvBox3" alignItems="Center" direction="Row" alignContent="SpaceBetween" justifyContent="SpaceBetween" displayInline="true" fitContainer="true">

                <Label class="label" text="Username" id="username" required="true" textAlign="Initial" />

                <Input class="inp" width="100%" id="inp_username" textAlign="Center" type="Email"  required="true" fieldWidth="100%" placeholder="Enter Your Username" />

            </VBox>

           

              <VBox id="vbox6" class="loginvBox6" alignItems="Center" direction="Row" alignContent="SpaceBetween" displayInline="true" fitContainer="true">

                <Label class="label" text="Mail-Id" required="true" id="mailid" textAlign="Initial" />

                <Input class="inp" width="100%" id="inp_mail" textAlign="Center" type="Email" required="true" fieldWidth="100%" placeholder="Enter Your mail" />

            </VBox>  

            <VBox id="vbox4" class="loginvBox4" alignItems="Center" direction="Row" alignContent="SpaceBetween" displayInline="true" fitContainer="true">

                <Label class="label" text="Password" required="true" id="password" textAlign="Initial" />

                <Input class="inp" width="100%" id="inp_password" type="Password" textAlign="Center"  required="true" fieldWidth="100%" placeholder="Enter Your Password" />

            </VBox>

              <VBox id="vbox5" class="loginvBox5" alignItems="Center" direction="Row" alignContent="SpaceBetween" displayInline="true" fitContainer="true">

                <Label class="label" text=" Re-Password  " required="true" id="repassword" textAlign="Initial" />

                <Input class="inp" width="100%" id="inp_repassword" textAlign="Center" type="Password" required="true" fieldWidth="100%" placeholder="Enter Your Password" />

            </VBox>    

            <VBox id="vbox7" class="loginvBox7" alignItems="Center" direction="Row" alignContent="SpaceBetween" displayInline="true" fitContainer="true">

                <Label class="label" text="Phone" required="true" id="phone" textAlign="Initial" />

                <Input class="inp" width="100%" id="inp_phone" textAlign="Center"  type="Number" required="true" fieldWidth="100%" placeholder="Enter Your Password" />

            </VBox>  
       

            <Button xmlns="sap.m" class="button1" text="Signup" id="login" type="Unstyled" press="newUser" width="100%" icon= "sap-icon://log" />

